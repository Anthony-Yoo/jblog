<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jblogBlog">

	<insert id="insertVo" parameterType="com.jblog.vo.BlogVo">
		<![CDATA[
			insert into blog 
			values (
			#{id},
			#{blogTitle},
			#{logoFile}		
			)		
		]]>
	</insert>
	<select id="selectId" parameterType="String" resultType="com.jblog.vo.BlogVo">
		<![CDATA[
				select id,
					   blogTitle,				   
					   logoFile
				from blog
				where id = #{id}
		]]>	
	</select>
	
</mapper>